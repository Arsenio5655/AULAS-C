#include <stdio.h>

int main() {
    
    int matriz [5][4] = {
        {2, 6, 9, 4},
        {3, 10, 2, 0},
        {4, 9, 9, 10},
        {9, 2, 7, 0},
        {10, 4, 7, 0}
    };
    
       int soma = 0;
       
    printf("Imprimindo os elementos da matriz:\n");
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {
        printf("%d ", matriz[i][j]);
        
    
        }
        printf("\n");
    }
     return 0;
}

